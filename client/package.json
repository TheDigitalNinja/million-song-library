{
  "name": "million_songs_library",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "bower install",
    "validate": "eslint src && jscs src",
    "test": "karma start --singleRun true",
    "autotest": "karma start",
    "webdriver-start": "webdriver-manager start",
    "protractor": "protractor protractor.conf.js",
    "build": "webpack --config webpack.prod.config.js && kss-node --config=kss-config.json",
    "styleguide": "kss-node --config=kss-config.json",
    "dev": "npm run validate && webpack-dev-server --port 3000 --content-base build/ --development --progress",
    "serve-prod": "http-server build -p 80",

    "parse-swagger-src": "node swagger/resolve.js -- index.yaml",
    "parse-account-edge-src": "node swagger/resolve.js -- account-edge/index.yaml",
    "parse-catalog-edge-src": "node swagger/resolve.js -- catalog-edge/index.yaml",
    "parse-login-edge-src": "node swagger/resolve.js -- login-edge/index.yaml",
    "parse-player-edge-src": "node swagger/resolve.js -- player-edge/index.yaml",
    "parse-ratings-edge-src": "node swagger/resolve.js -- ratings-edge/index.yaml",
    "generate-swagger-html": "npm run parse-swagger-src && java -jar ../common/swagger-codegen-cli.jar generate -i swagger/api/swagger/swagger.yaml -l html -o ../docs/swagger -c swagger/package.json",

    "build-server": "cd ../server && mvn -f build.xml clean compile",
    "account-edge-server": "cd ../server/msl-account-edge && mvn -P dev clean jetty:run",
    "catalog-edge-server": "cd ../server/msl-catalog-edge && mvn -P dev clean jetty:run",
    "login-edge-server": "cd ../server/msl-login-edge && mvn -P dev clean jetty:run",
    "ratings-edge-server": "cd ../server/msl-ratings-edge && mvn -P dev clean jetty:run",

    "serve-mock": "npm run parse-swagger-src && swagger project start --mock swagger"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "^1.4.3",
    "babel": "4.7.16",
    "babel-core": "^5.8.3",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.3",
    "bourbon": "^4.2.5",
    "bourbon-neat": "^1.7.2",
    "bourbon-sass-loader": "^0.1.0",
    "bower": "^1.4.1",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.15.5",
    "es5-shim": "^4.1.10",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.0",
    "http-server": "^0.8.0",
    "image-webpack-loader": "^1.6.1",
    "imagemin": "^3.2.0",
    "isparta": "^3.1.0",
    "isparta-loader": "^1.0.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.4",
    "jscs": "^2.1.1",
    "karma": "^0.13.9",
    "karma-babel-preprocessor": "^5.2.1",
    "karma-coverage": "git://github.com/douglasduteil/karma-coverage#next",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "kss": "^2.1.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "less-plugin-clean-css": "^1.5.0",
    "ng-annotate-loader": "0.0.6",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.3.3",
    "phantomjs": "^1.9.17",
    "protractor": "^2.2.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.3",
    "swagger": "^0.6.3",
    "swagger-express-mw": "0.0.4",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1",
    "yargs": "^3.17.1"
  },
  "dependencies": {
    "angular": "^1.4.3",
    "angular-animate": "^1.4.7",
    "angular-aria": "^1.4.7",
    "angular-cookies": "^1.4.3",
    "angular-material": "^0.11.4",
    "angular-messages": "^1.4.8",
    "angular-permission": "^1.0.6",
    "angular-toastr": "^1.6.0",
    "angular-ui-router": "^0.2.15",
    "assert": "^1.3.0",
    "connect": "^3.2.0",
    "cookie-parser": "^1.4.0",
    "font-awesome": "^4.3.0",
    "istanbul": "^0.3.22",
    "jquery": "^1.11.3",
    "json-refs": "^1.3.0",
    "json2yaml": "^1.0.3",
    "lodash": "^3.10.0",
    "slick-carousel": "1.5.8",
    "swagger-tools": "0.9.7",
    "yaml-js": "^0.1.3"
  }
}
